    "class rectangle:\n",
    "    lenght = 0\n",
    "    breath = 0\n",
    "    def __init__(self,l,w):\n",
    "        self.lenght = l\n",
    "        self.breath = w\n",
    "    def area(self,obj):\n",
    "        return self.lenght*self.breath\n",
    "ob = rectangle(3,2)\n",
    "ob.area(ob)     "
